<div class="outer">
    <%= form_for :order, url: "/confirmation" do |f| %>
        <p class="text">
            <%= f.label :First_Name %> <%= f.text_field :First_name %>  <%= f.label :Last_Name %> <%= f.text_field :Last_Name %>
        </p>
        <%= submit_tag "Submit" %>
    <% end %>
</div>